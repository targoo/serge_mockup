@linei:@template"?/* Extra small devices (phones, less than 768px) */
.navigation {
  background-color: $navigation-background-color;
  height: 100%;
  left: -70%;
  width: 70%;
  position: absolute;
  top: 0;

  // First level.
  .nav {
    display: block;

    li {
      display: block;
      text-align: right;

      .nolink, a {
        display: block;
        font-size: 14px;
        font-weight: bold;
        padding: 10px 30px;
        @include unstyled-link();
        color: $white;

        &:hover {
          color: $blue;
        }
      }

      &:hover {
        ul {
          border-right: 2px solid $blue;
        }
      }

      // Second level.
      ul {
        border-right: 2px solid $white;
        left: auto ;
        margin: 0px 15px 0px 0px ;
        position: relative;
        top: auto;
        width: auto;

        li {

          .nolink, a {
            font-size: 14px;
          }

          // Third level.
          ul {
            display: block;
            left: auto;
            margin: 0px 15px 0px 0px;
            position: relative;
            top: auto;
            width: auto;
          }
        }
      }
    }
  }
}

/* Small devices (tablets, 768px and up) */
@media (min-width: 768px) {
  .navigation {
    background-color: $navigation-background-color;
    height: 100%;
    left: 0%;
    position: static;
    width: 100%;
    line-height: 1;

    .nav {
      @include clearfix-normal;
      display: block;
      float: left;
      list-style: none;

      li {
        background-color: transparent;
        float: left;
        position: relative;
        clear: none;

        .nolink, a {
          @include transition(none);
          background-color: transparent;
          border-left: 0px;
          border-top: 2px solid transparent;
          color: $navigation-link-color;
          display: block;
          font-size: 16px;
          font-weight: normal;
          line-height: 1;
          padding: 21px 20px 23px 20px;
          white-space: nowrap;

          &:hover {
            background-color: $navigation-link-hover-background-color;
            border-top: 2px solid $navigation-link-border-color;
            color: $navigation-link-color-hover;
            text-decoration: none;
          }

          &.active {
            border-top: 2px solid $navigation-link-border-color;
          }

        }

        &.menuparent {

          > .nolink, > a {
            background-image: url('../img/arrow-bottom-black.png');
            background-position: 90% center;
            background-repeat: no-repeat;
            background-size: 11px 8px;
            padding-right: 40px;
          }

          li {

            &.menuparent {

              > .nolink, > a {
                background-image: url('../img/arrow-right-white.png');
                background-size: 8px 11px;
              }
            }
          }
        }

        &:hover {
          > ul {
            visibility: visible;
          }
          > .nolink, > a {
            background-color: $navigation-link-hover-background-color;
            border-top: 2px solid $navigation-link-border-color;
            color: $navigation-link-color-hover;
            text-decoration: none;

            &.menuparent {
              background-image: url('../img/arrow-bottom-white.png');
            }
          }
        }

        ul {
          @include background-image(none);
          @include border-radius(0px);
          background-color: $navigation-sub-background-color;
          margin: 62px 0px 0px 0px;
          position: absolute;
          padding: 0px; top: 0px;
          visibility: hidden;
          width: 220px;
          z-index: 999;
          li { float: none; display: block; min-width: 100%;
            .nolink, a {
              @include transition(background-color .1s ease-in, padding-left .1s ease-in);
              border-top: 0px !important;
              color: $navigation-sub-link-color !important;
              display: block;
              font-size: 14px;
              padding-right: 20px;
              padding-bottom: 15px;
              padding-top: 15px;

              &.menuparent {
                background-image: url('../img/arrow-right-white.png');
                background-position: 90%;
                background-size: 8px 11px;
                background-repeat: no-repeat;

                &:hover {
                  background-image: url('../img/arrow-right-white.png');
                  background-size: 8px 11px;
                }
              }

              &:hover {
                background-color: $blue;
                padding-left: 25px;
              }
            }

            ul {
              left: 218px !important;
              border-right: 0 none;
              display: block;
              margin: 0;
              position: absolute;
              top:0;
              visibility: hidden;
              width: 220px;

            }
          }
        }
      }
    }

    .site-search { float: right; margin: 13px 20px 0px 0px;
      input { @include border-radius(0px); border-right: 0px; width: 146px; }
      button { @include background-image(none); @include border-radius(0px); @include box-shadow(none); @include text-shadow(none);
        background-color: #fff; background-image: url('../img/icons/search.png'); background-position: center center; background-repeat: no-repeat; background-size: 13px 13px; border: 1px solid $gray-medium; border-left: 0px; display: inline-block; margin: 0px 0px 0px -5px; padding: 6px 15px; vertical-align: top; text-indent: -9999px;
        @media (-webkit-min-device-pixel-ratio: 2), (-moz-min-device-pixel-ratio: 2) {
          background-image: url('../img/icons/search@2x.png');
        }
      }
      .control-group { display: inline-block; margin: 0px; }
    }

  }
}

